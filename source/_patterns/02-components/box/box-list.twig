{% embed "modules-box" %}
  {% block header %}
    {% include "elements-heading" with { heading: boxList.heading } %}
  {% endblock %}
  {% block content %}
    {% if boxList.buttonIconLink %}
      {% include "modules-button-icon-link" with { buttonIconLink: boxList.buttonIconLink } %}
    {% endif %}
    {% include "modules-content-list" with { list: boxList.list } %}
    {% if boxList.boxLink %}
      {% include "modules-box-link" with { boxLink: boxList.boxLink } %}
    {% endif %}
  {% endblock %}
  {% block footer %}
    {% if boxList.boxPager %}
      {% embed "modules-grid-two-thirds-centered" %}
        {% block items %}
          {% embed "modules-grid-item-centered" %}
            {% block content %}
              {% include "modules-box-pager" with { boxPager: boxList.boxPager } %}
            {% endblock %}
          {% endembed %}
        {% endblock %}
      {% endembed %}
    {% endif %}
  {% endblock %}
{% endembed %}
