{% embed "modules-box" %}
  {% block header %}
    {% include "elements-heading" with { heading: boxList.heading } %}
  {% endblock %}
  {% block content %}
    {% include "modules-button-link" with { buttonLink: boxList.buttonLink } %}
    {% include "modules-list" with { list: boxList.list } %}
